<% include ../partials/head %>


    <table class="table">
        <thead>
            <tr>
                <th>Title</th>
                <th>Fname</th>
                <th>Lname</th>
                <th>SID</th>
            </tr>
        </thead>
        <tbody>
          <% rows.forEach(function(rows){ %>
            <tr>
                <td><%= rows.title %></td>
                <td scope="row"><%= rows.firstname %></td>
                <td><%= rows.lastname %></td>
                <td><%= rows.sid %></td>
                 
            </tr>
            <% }); %>
        </tbody>
    </table>


<% include ../partials/footer %>