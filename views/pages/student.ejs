<% include ../partials/head %>


    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Email</th>
                <th>Password</th>
            </tr>
        </thead>
        <tbody>
          <% rows.forEach(function(rows){ %>
            <tr>
                <td scope="row"><%= rows.firstname %></td>
                <td><%= rows.lastname %></td>
                <td><%= rows.sid %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>


<% include ../partials/footer %>